{
  "name": "@akb2/math",
  "version": "1.0.1",
  "description": "The little mathematic library for TypeScript",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "math",
    "typescript"
  ],
  "author": "Андрей",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akb2/math.git"
  },
  "bugs": {
    "url": "https://github.com/akb2/math/issues"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean --out-dir dist",
    "prepublishOnly": "npm run build"
  }
}